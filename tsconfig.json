{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/seo/*": [
        "seo/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/libs/*": [
        "libs/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/layouts/*": [
        "layouts/*"
      ],
      "@/db/*": [
        "db/*"
      ],
    },
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}